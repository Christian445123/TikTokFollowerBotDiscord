# Beispiel .env
DISCORD_TOKEN=      # Discord Bot Token
GUILD_ID=           # z.B. 123456789012345678

# Allgemeiner Channel (optional). Wird genutzt, wenn keine separaten CHANNEL_ID_TIKTOK/INSTAGRAM gesetzt sind.
CHANNEL_ID=0

# Optional: separate Channel-IDs pro Plattform (Ã¼berschreiben CHANNEL_ID)
CHANNEL_ID_TIKTOK=0
CHANNEL_ID_INSTAGRAM=0

# Plattform-Benutzernamen (ohne @)
TIKTOK_USERNAME=dein_tiktok_name
INSTAGRAM_USERNAME=dein_instagram_name

# Aktivierung: true/false (optional)
# Wenn nicht gesetzt, werden Plattformen automatisch aktiviert, wenn USERNAME gesetzt ist.
ENABLE_TIKTOK=true
ENABLE_INSTAGRAM=true

# Default 4 Stunden (14400 Sekunden)
UPDATE_INTERVAL=14400

# Optional: eigener User-Agent
USER_AGENT=Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/120.0.0.0 Safari/537.36